<!-- change <target dev='sdn'... to any sd(number) you like as long as it is unique within all target disks in your xml -->
<devices>
  <disk type='file' device='cdrom'>
    <driver name='qemu' type='raw'/>
    <source file='file.iso'/>
    <target dev='sdn' bus='sata'/>
    <readonly/>
  </disk>
</devices>
